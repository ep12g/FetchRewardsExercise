package test;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class FetchChallenge {

	public static void main(String[] args) {
		
		System.setProperty("webdriver.chrome.driver","C:\\Users\\Elie\\Desktop\\Fetch Rewards Coding Exercise\\chromedriver.exe");
		WebDriver driver = new ChromeDriver();
		
        String baseUrl = "http://ec2-54-208-152-154.compute-1.amazonaws.com/";
        //String expectedTitle = "Welcome: Mercury Tours";
        //String actualTitle = "";
        
        driver.get(baseUrl);
        
        driver.findElement(By.xpath("//*[@id='left_0']")).sendKeys("0");
        driver.findElement(By.xpath("//*[@id='left_1']")).sendKeys("1");
        driver.findElement(By.xpath("//*[@id='left_2']")).sendKeys("2");
        driver.findElement(By.xpath("//*[@id='left_3']")).sendKeys("3");
        
        driver.findElement(By.xpath("//*[@id='right_0']")).sendKeys("4");
        driver.findElement(By.xpath("//*[@id='right_1']")).sendKeys("5");
        driver.findElement(By.xpath("//*[@id='right_2']")).sendKeys("6");
        driver.findElement(By.xpath("//*[@id='right_3']")).sendKeys("7");
	}
}
